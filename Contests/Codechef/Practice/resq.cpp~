#include<stdio.h>
#include <iostream>
#include <vector>
#include <math.h>
#include <algorithm>

using namespace std;
vector<long long> n;
int main(){
	
	int t,d1;
	long long e;
	t=getchar_unlocked();
	t=t-'0';
	cout<<t;
	while(t--){
	scanf("%d",&d1);
	e=getchar_unlocked();
	e=e-'0';
	for(int i=1;i*i<=e;i++){
		if(e%i==0)
			{
				n.push_back(abs(i-e/i) );
				
			}		
		}
	sort(n.begin(),n.end());
	printf("%lld\n",n[0]);		
	n.clear();	
	}
	
	
	

	return 0;
}
