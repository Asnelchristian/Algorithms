#include<stdio.h>
using namespace std;
unsigned long long f(int n)
{	
	if(n<=0)
		return 1;
	return n*f(n-1);
}
 
int main()
{
    int n,t;
	unsigned long long fact;
    scanf("%d",&t);
    while(t--){
    
    scanf("%d",&n);
    fact=f(n);  // if(N-R>R)?C(N,R):C(N,N-R)
    printf("%llu\n",fact);
    }
    
    return 0;
}
